<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/background">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="5dp"
        android:gravity="center">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:fontFamily="@font/nerkooneregular"
            android:text="@string/child_s_scores"
            android:textColor="@color/black"
            android:textSize="50dp"></TextView>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:layout_marginTop="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:fontFamily="@font/nerkooneregular"
            android:text="@string/shapes"
            android:textColor="@color/black"
            android:textSize="35dp"></TextView>

        <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_gravity="center">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="25dp"
            android:textColor="@color/black"
            android:text="@string/level_1"
            android:textStyle="bold"
            android:fontFamily="@font/nerkooneregular"
            android:layout_marginRight="5dp">
        </TextView>

        <TextView
            android:id="@+id/textView_shapesLevel1Score"
            android:layout_width="200dp"
            android:layout_marginLeft="10dp"
            android:layout_height="wrap_content"
            android:hint="@string/score"
            android:padding="5dp"
            android:textSize="25dp" />
    </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="center">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="25dp"
                android:textColor="@color/black"
                android:text="@string/level_2"
                android:textStyle="bold"
                android:fontFamily="@font/nerkooneregular"
                android:layout_marginRight="5dp">

            </TextView>
            <TextView
                android:id="@+id/editText_shapesLevel2Score"
                android:layout_width="200dp"
                android:layout_marginLeft="10dp"
                android:layout_height="wrap_content"
                android:hint="@string/score"
                android:padding="5dp"
                android:textSize="25dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_marginTop="5dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="@color/white"
                android:fontFamily="@font/nerkooneregular"
                android:text="@string/sea_world"
                android:textColor="@color/black"
                android:textSize="35dp">
            </TextView>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_gravity="center">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="25dp"
                    android:textColor="@color/black"
                    android:text="@string/level_1"
                    android:textStyle="bold"
                    android:fontFamily="@font/nerkooneregular"
                    android:layout_marginRight="5dp">
                </TextView>

                <TextView
                    android:id="@+id/editText_seaWorldLevel1Score"
                    android:layout_width="200dp"
                    android:layout_marginLeft="10dp"
                    android:layout_height="wrap_content"
                    android:hint="@string/score"
                    android:padding="5dp"
                    android:textSize="25dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_gravity="center">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="25dp"
                    android:textColor="@color/black"
                    android:text="@string/level_2"
                    android:textStyle="bold"
                    android:fontFamily="@font/nerkooneregular"
                    android:layout_marginRight="5dp">

                </TextView>

                <TextView
                    android:id="@+id/editText_seaWorldLevel2Score"
                    android:layout_width="200dp"
                    android:layout_marginLeft="10dp"
                    android:layout_height="wrap_content"
                    android:hint="@string/score"
                    android:padding="5dp"
                    android:textSize="25dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="5dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:background="@color/white"
                    android:fontFamily="@font/nerkooneregular"
                    android:text="@string/animal_world"
                    android:textColor="@color/black"
                    android:textSize="35dp"></TextView>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_gravity="center">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="25dp"
                        android:textColor="@color/black"
                        android:text="@string/level_1"
                        android:textStyle="bold"
                        android:fontFamily="@font/nerkooneregular"
                        android:layout_marginRight="5dp">
                    </TextView>

                    <TextView
                        android:id="@+id/textView_animalWorldLevel1Score"
                        android:layout_width="200dp"
                        android:layout_marginLeft="10dp"
                        android:layout_height="wrap_content"
                        android:hint="@string/score"
                        android:padding="5dp"
                        android:textSize="25dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_gravity="center">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="25dp"
                        android:textColor="@color/black"
                        android:text="@string/level_2"
                        android:textStyle="bold"
                        android:fontFamily="@font/nerkooneregular"
                        android:layout_marginRight="5dp">

                    </TextView>

                    <TextView
                        android:id="@+id/editText_animalWorldLevel2Score"
                        android:layout_width="200dp"
                        android:layout_marginLeft="10dp"
                        android:layout_height="wrap_content"
                        android:hint="@string/score"
                        android:padding="5dp"
                        android:textSize="25dp" />
                </LinearLayout>

        <Button
            android:id="@+id/button_logout"
            android:layout_width="140dp"
            android:layout_height="75dp"
            android:layout_gravity="center"
            android:layout_marginTop="10dp"
            android:text="@string/logout"
            android:textSize="25sp"></Button>
    </LinearLayout>

    </LinearLayout>

    </LinearLayout>
    </LinearLayout>
</ScrollView>